<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="INFO">
    <Appenders>
        <Console name="LogToConsole" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS}[%t] %-5level %logger{36} - %msg%n </Pattern >
            </PatternLayout>
        </Console>
    </Appenders>
    <Logger>
        <Root level="INFO">
            <AppenderRef ref="LogToConsole"/>
        </Root>
    </Logger>
</Configuration>

